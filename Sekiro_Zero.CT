  [SLASH_TRANS_READY_ACTIVE] = slash_transition_ready_active,
  [SLASH_S_ACTIVE] = slash_state_active, --ACTIVE
  [SLASH_TRANS_ACTIVE_WAITING] = slash_transition_active_waiting,
